{
  "name": "@tantanok221/agentbudget",
  "publishConfig": {
    "access": "public"
  },
  "version": "1.0.1",
  "description": "Personal budgeting CLI (envelopes, transactions, schedules) backed by SQLite/libsql.",
  "main": "dist/cli.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "dev": "tsx src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "echo \"(todo)\"",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "prepare": "npm run build",
    "postbuild": "node scripts/copy-migrations.mjs && chmod +x dist/cli.js || true"
  },
  "keywords": [
    "budget",
    "ynab",
    "envelope",
    "finance",
    "cli"
  ],
  "author": "Tan Kai",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tantanok221/agentbudget.git"
  },
  "bugs": {
    "url": "https://github.com/Tantanok221/agentbudget/issues"
  },
  "homepage": "https://github.com/Tantanok221/agentbudget#readme",
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "files": [
    "dist/**",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "agentbudget": "dist/cli.js"
  },
  "dependencies": {
    "@libsql/client": "^0.17.0",
    "commander": "^14.0.3",
    "dotenv": "^17.2.4",
    "drizzle-orm": "^0.45.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.2",
    "drizzle-kit": "^0.31.8",
    "execa": "^9.6.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "rimraf": "^6.0.1"
  }
}
